{
    "policies":{
        "Extensions":{
            "Install":[
                {% for extension in extensions %}
                "https://addons.mozilla.org/firefox/downloads/latest/{{ extension }}/latest.xpi"{% if not loop.last %},{% endif %}
                {% endfor %}
            ]
        }
    "ExtensionSettings": {
      "*": {
        "allowed_types": ["extension", "theme"]
    }
}
